<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Version>2.1.1.2</Version>
    <Authors>tangdabin</Authors>
    <Company>tdb</Company>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Description>consul服务注册与发现帮助类</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageTags>tdb</PackageTags>
    <PackageProjectUrl>https://github.com/dabintang/tdb.common</PackageProjectUrl>
  </PropertyGroup>

  <PropertyGroup>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageReleaseNotes>1、替换json序列化工具：Newtonsoft.Json -&gt; System.Text.Json
2、升级组件</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Consul" Version="1.6.10.7" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Abstractions" Version="6.0.0" />
    <PackageReference Include="Microsoft.Extensions.Hosting.Abstractions" Version="6.0.0" />
  </ItemGroup>

</Project>
