  <PropertyGroup>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>E:\tdb\github\svn\tdb.common\tdb.common\tdb.common.xml</DocumentationFile>
  </PropertyGroup>

</Project>
